{
   "manifest_version": 2,
   "name": "Finna Recommends",
   "version": "1",
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "content.js"
         ]
      }
   ],
   "background": {
      "scripts": [
         "background.js"
      ]
   },
   "browser_action": {
      "default_icon": "finna.png",
      "default_popup": "sketch/index.html",
      "default_title": "Find Recommendations"
   }
}